        <style>
            /* Research Publications Styles */
            .research-publications {
                padding: 2rem 0;
            }

            .research-publications__stats {
                display: grid;
                grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
                gap: 1.5rem;
                margin: 2rem 0;
            }

            .research-publications__stat-card {
                background: rgba(30, 30, 35, 0.6);
                padding: 1.5rem;
                border-radius: 12px;
                text-align: center;
                box-shadow: 0 10px 20px rgba(0, 0, 0, 0.3);
                border: 1px solid rgba(255, 255, 255, 0.1);
                transition: transform 0.3s ease;
            }

            .research-publications__stat-card:hover {
                transform: translateY(-5px);
            }

            .research-publications__stat-number {
                font-size: 2.5rem;
                font-weight: 700;
                color: #fff;
                margin-bottom: 0.5rem;
            }

            .research-publications__stat-label {
                color: #bbb;
                font-size: 0.9rem;
            }

            .research-publications__filters {
                display: flex;
                justify-content: center;
                gap: 1rem;
                margin: 2rem 0;
            }

            .research-publications__filter {
                padding: 0.6rem 1.2rem;
                border: none;
                border-radius: 30px;
                background: rgba(40, 40, 45, 0.7);
                color: #ddd;
                cursor: pointer;
                transition: all 0.3s ease;
            }

            .research-publications__filter.active {
                background: linear-gradient(135deg, #4776E6, #8E54E9);
                color: #fff;
            }

            .research-publications__grid {
                display: grid;
                grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
                gap: 2rem;
                margin: 2rem 0;
            }

            .research-publications__card {
                background: rgba(30, 30, 35, 0.6);
                border-radius: 12px;
                padding: 1.5rem;
                box-shadow: 0 10px 20px rgba(0, 0, 0, 0.3);
                border: 1px solid rgba(255, 255, 255, 0.1);
                transition: transform 0.3s ease;
            }

            .research-publications__card:hover {
                transform: translateY(-5px);
            }

            .research-publications__card-header {
                margin-bottom: 1rem;
            }

            .research-publications__card-title {
                font-size: 1.1rem;
                color: #fff;
                margin-bottom: 1rem;
                line-height: 1.4;
            }

            .research-publications__card-badges {
                display: flex;
                gap: 0.5rem;
                flex-wrap: wrap;
            }

            .research-publications__badge {
                padding: 0.3rem 0.8rem;
                border-radius: 20px;
                font-size: 0.8rem;
                font-weight: 500;
            }

            .research-publications__badge.type {
                background: rgba(71, 118, 230, 0.2);
                color: #4776E6;
            }

            .research-publications__badge.impact {
                background: rgba(142, 84, 233, 0.2);
                color: #8E54E9;
            }

            .research-publications__badge.quartile {
                background: rgba(255, 107, 107, 0.2);
                color: #ff6b6b;
            }

            .research-publications__card-body {
                color: #bbb;
                font-size: 0.9rem;
            }

            .research-publications__authors {
                color: #fff;
                margin-bottom: 0.5rem;
            }

            .research-publications__journal {
                margin-bottom: 0.5rem;
            }

            .research-publications__year {
                margin-bottom: 0.5rem;
            }

            .research-publications__status {
                font-style: italic;
            }

            .research-publications__load-more {
                display: block;
                margin: 2rem auto;
                padding: 0.8rem 2rem;
                border: none;
                border-radius: 30px;
                background: linear-gradient(135deg, #4776E6, #8E54E9);
                color: #fff;
                font-weight: 500;
                cursor: pointer;
                transition: all 0.3s ease;
            }

            .research-publications__load-more:hover {
                transform: translateY(-2px);
                box-shadow: 0 5px 15px rgba(71, 118, 230, 0.4);
            }
        </style>
